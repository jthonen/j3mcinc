{
  "name": "new-canaan-racquet-club-tennis-client",
  "version": "1.0.0",
  "description": "New Canaan Racquet Club Tennis Website",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config ./webpack/dev.config.js --mode development",
    "build": "webpack --config ./webpack/base.config.js --host $IP --mode production",
    "eslint-fix": "eslint --fix \"src/**/*.js\"",
    "test": "npm run test:unit && npm run test:snapshot && npm run storybook",
    "test:unit": "mocha --require @babel/register --require ./test/helpers.config.js --require ./test/dom.js --require ignore-styles src/**/*.test.js --exit",
    "test:unit:watch": "npm run test:unit --watch",
    "test:snapshot": "jest --config ./test/jest.config.json",
    "test:snapshot:watch": "npm run test:snapshot --watch",
    "test:coverage": "npm run test:snapshot --coverage --colors",
    "test:cypress": "cd .. && npm run test:cypress",
    "cypress": "cypress run",
    "cypress:open": "cypress open",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/test/jest.setup.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@storybook/addon-actions": "^5.0.11",
    "@storybook/addon-knobs": "^5.0.11",
    "@storybook/addon-storyshots": "^5.0.11",
    "@storybook/addon-storyshots-puppeteer": "^5.0.11",
    "@storybook/addon-viewport": "^5.0.11",
    "@storybook/react": "^5.0.11",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-require-context-hook": "^1.0.0",
    "chai": "^4.2.0",
    "css-loader": "^2.1.1",
    "cypress": "^3.2.0",
    "cypress-plugin-retries": "^1.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "eslint": "^5.15.2",
    "eslint-config-react": "^1.1.7",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "ignore-styles": "^5.0.1",
    "jest": "^24.7.1",
    "jsdom": "^14.0.0",
    "mocha": "^6.1.4",
    "react-test-renderer": "^16.8.6",
    "sinon": "^7.3.2",
    "start-server-and-test": "^1.9.0",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-router-dom": "^4.3.1"
  }
}
